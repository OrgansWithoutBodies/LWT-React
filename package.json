{
  "name": "lwt-react",
  "homepage": "/lwt",
  "version": "1.0.0",
  "description": "LWT React Port",
  "main": "./out/main/index.js",
  "scripts": {
    "patch": "npx patch-package",
    "dev": "yarn dev:localstorage",
    "dev:localstorage": "VITE_LWT_PERSIST=LOCALSTORAGE vite",
    "dev:rest": "VITE_LWT_PERSIST=REST vite",
    "dev:electron:localstorage": "VITE_LWT_PERSIST=LOCALSTORAGE electron-vite dev -w",
    "dev:electron:sqlite": "VITE_LWT_PERSIST=SQLITE electron-vite dev -w",
    "dev:electron:rest": "VITE_LWT_PERSIST=REST electron-vite dev -w",
    "build": "yarn run build:localstorage",
    "build:rest": "VITE_LWT_PERSIST=REST tsc && vite build",
    "build:skipTS": "VITE_LWT_PERSIST=LOCALSTORAGE vite build",
    "build:localstorage": "VITE_LWT_PERSIST=LOCALSTORAGE tsc && vite build",
    "build:electron:sqlite": "VITE_LWT_PERSIST=SQLITE tsc && electron-vite build",
    "build:electron:localstorage": "VITE_LWT_PERSIST=LOCALSTORAGE tsc && electron-vite build",
    "build:electron:rest": "VITE_LWT_PERSIST=REST tsc && electron-vite build",
    "preview:electron": "electron-vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix --ext ts,tsx",
    "preview": "vite preview",
    "test": "jest",
    "deps": "npx depcruise src --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
    "start": "webpack-dev-server --mode development",
    "generateTypeSchemas": "npx openapi-typescript src/shared/apis/src/** --output src/shared/apis/out/",
    "mapIcons": "rm src/icons/index.ts && npx tsc cli/createIconMap.ts && node cli/createIconMap.js"
  },
  "author": "v",
  "license": "ISC",
  "dependencies": {
    "@datorama/akita": "^6.2.4",
    "@floating-ui/react": "^0.19.2",
    "anki-apkg-export": "^4.0.3",
    "anki-reader": "^0.3.0",
    "assert": "^2.0.0",
    "axios": "^1.6.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "genanki": "^0.1.1",
    "node-gzip": "^1.1.2",
    "papaparse": "^5.4.1",
    "process": "^0.11.10",
    "rambdax": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.18.0",
    "react-split-pane": "^0.1.92",
    "rxjs": "^7.4.0",
    "sqlite": "^5.1.1",
    "sqlite-electron": "^2.2.8",
    "sqlite3": "^5.1.6",
    "stream": "^0.0.2",
    "styled-components": "^5.3.6",
    "superstruct": "^1.0.3",
    "typedoc": "^0.25.4",
    "util": "^0.12.5",
    "vite": "^4.5.0",
    "yarn": "^1.22.19",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.16.3",
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.11",
    "@types/node-gzip": "^1.1.0",
    "@types/react-modal": "^3.16.3",
    "@types/papaparse": "^5.3.13",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/sqlite3": "^3.1.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "babel-loader": "^8.2.3",
    "dependency-cruiser": "^15.5.0",
    "electron": "^27.1.2",
    "electron-packager": "^17.1.2",
    "electron-vite": "^1.0.29",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "openapi-typescript": "^6.7.1",
    "playwright": "^1.40.1",
    "prettier": "^2.4.1",
    "swagger2openapi": "^7.0.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "tsdoc": "^0.0.4",
    "typescript": "^5.0.2",
    "vite": "^4.5.0"
  },
  "browser": {
    "assert": false
  }
}
